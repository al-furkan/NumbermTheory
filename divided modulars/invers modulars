#include<bits/stdc++.h>
using namespace std;
int  power(int base ,int n,int m){
int res=1;
while(n){
    if(n%2==0){
        res=(res*base)%m;
        n--;
    }
    else{
        base=(base*base)%m;
        n/=2;
    }
    return res%m;
}


}
int main()
{
    int a,b,m;
    cin>>a>>b>>m;//(a/b)%m
    int x = power(b,m-2,m);
    int ans=(a*x)%m;
    cout<<ans<<endl;
    return 0;
}
