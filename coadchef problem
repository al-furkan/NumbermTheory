#include<bits/stdc++.h>
using  namespace  std;
#define mod 1000000007
typedef long long int ll;
typedef unsigned long long int  llu;

ll power( ll base,ll n)
{
    ll res = 1;
    while(n)
    {
        if(n%2==1)
        {
            res*=base;
            n--;
        }
        else
        {
            base*=base;
            n/=2;
        }
    }
    return res;
}

ll phi(int n)
{
    ll ans=1;
    for(int i=2; i*i<=n; i++)
    {
        if(n%i==0)
        {
            int cnt=0;
            while(n%i==0)
            {
                cnt++;
                n/=i;

            }
            ans*=((power(i,cnt+1)-1)/(i-1));

        }
    }
    if(n>1)
    {
        ans*=((power(n,2)-1)/(n-1));
    }
    return ans;
}

int main()
{
    int n;
    cin>>n;
    while(n--)
    {
        ll x,a,b;
        cin>>x>>a>>b;
        ll z = x*b;
        if(z%a==0 &&(phi(z/a)==x))
        {
            cout<<z/a<<endl;
        }
        else cout<<-1<<endl;
    }
}
