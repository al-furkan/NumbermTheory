int gcd(int a, int b){
  if(b==0)return a;
  else return gcd(b,a%b);
}
int main()
{
    int a;
    cin>>a;
    while(a--)
    {
        int a,b;
        cin>>a>>b;
        int ans =gcd(a,b);
       cout<<ans<<endl;

    }
    return 0;
}
