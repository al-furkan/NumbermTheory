#include<bits/stdc++.h>
using namespace std;
void mod(string s,int m){
 int ans=0;
 for(int i=0;i<s.size();i++){
    ans=ans*10+s[i]-'0';
    ans=ans%m;
 }
 if(ans%m==0)cout<<"yes divizor"<<endl;
 else cout<<"No"<<endl;
}
int main()
{
    int t,m;
    cin>>t;
    while(t--){
     string s;
     cin>>s>>m;
     mod(s,m);
    }
  return 0;
}
